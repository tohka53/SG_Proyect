<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

    <link rel="stylesheet" href="@Url.Content("~/Content/AdminLte/css/font-awesome.min.css")">
    <link rel="stylesheet" href="@Url.Content("~/Content/AdminLte/css/ionicons.min.css")">
    <link rel="stylesheet" href="@Url.Content("~/Content/AdminLte/css/AdminLTE.css")">
    <link rel="stylesheet" href="@Url.Content("~/Content/AdminLte/css/skins/skin-red.css")">




</head>
<body class="skin-red">

    <!-- Left side column. contains the logo and sidebar -->   <!-- Left side column. contains the logo and sidebar -->
    <aside class="main-sidebar">
        <!-- sidebar: style can be found in sidebar.less -->
        <section class="sidebar">
            <!-- Sidebar user panel (optional) -->
            <div class="user-panel">
                <div class="pull-left image">

                    <img src="~/Content/Content/AdminLte/img/Imagen6.JPG" />
                </div>
                <div class="pull-left info">
                    <p>@Html.Encode(User.Identity.Name.ToUpper())</p>
                    <!-- Status -->
                    <a href="#"><i class="fa fa-circle text-success"></i> MECG is Online</a>
                </div>
            </div>
            <!-- Sidebar Menu -->
            <ul class="sidebar-menu">
                <!-- Optionally, you can add icons to the links -->
                <li>@Html.ActionLink("Pagina de Inico", "Index", "Home")</li>
                <li>@Html.ActionLink("Acerca de Nosotros", "About", "Home")</li>

                <li>@Html.ActionLink("Contactanos", "Contact", "Home")</li>
                <li>@Html.ActionLink("Productos", "Index", "Productoes")</li>
                <li>@Html.ActionLink("Clientes", "Index", "Clientes")</li>
               



    </ul><!-- /.sidebar-menu -->
</section>
        <!-- /.sidebar -->
    </aside>
    <!--**********************************-->
   
    <!--**********************************-->

    <div class="content-wrapper">
        <!-- Content Header (Page header) -->
        <section class="content-header"></section>
        <!-- Main content -->
        <section class="content">
            @RenderBody()
        </section><!-- /.content -->
    </div><!-- /.content-wrapper -->
    <!-- Main Footer -->
    <footer class="main-footer">     
        <!-- Default to the left -->
        <div class="row">
            <!--<div class="hidden-sm hidden-xs col-md-1 col-lg-1">&nbsp;</div>-->
            <div class="col-xs-12 col-sm-12 col-md-10 col-lg-10">
                <strong>Copyright &copy; 2018 <a href="https://github.com/tohka53">MECG53</a>.</strong> All rights reserved.
            </div>
            <div class="hidden-sm hidden-xs col-md-1 col-lg-1">&nbsp;</div>
            <!-- Your Page Content Here -->
        </div><!-- /.row -->
    </footer>



    <!--- COMIENZA EL CODIGO HTML ORIGINAL --->
    @*@Scripts.Render("~/bundles/jquery")*@
    @*@Scripts.Render("~/bundles/bootstrap")*@


    @Scripts.Render("~/Scripts/jquery-1.10.2.min.js")
    @Scripts.Render("~/Scripts/bootstrap.js")
    @Scripts.Render("~/Content/AdminLte/js/app.js")
    @Scripts.Render("~/bundles/jqueryval")
    @RenderSection("scripts", required: false)
</body>
</html>
